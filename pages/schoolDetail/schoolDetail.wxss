/*折线图*/
ec-canvas {
    width: 100%;
    height: 100%;
    z-index: -1;
}


.canvas-container {
    position: relative;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 400rpx;

    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;

}

.bar-container {
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    font-size: 25rpx;
    margin-bottom: 30rpx;
