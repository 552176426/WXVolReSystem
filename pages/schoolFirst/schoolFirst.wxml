                        </view>


                    </view>

                </view>

                <!--下-->
                <view class="list-special">
                    <text>展开专业列表</text>
                </view>
            </view>
        </swiper-item>


        <!--更多-->
          <swiper-item class="swiper-item">
            <view class="swiper-item-view" wx:for="{{schools.sortedList}}" wx:key="index">
                <!--上-->
                <view class="swiper-item-view-up">
                    <view class="left">
                        <!--<view class="mark-view" style="background-color: #1888f7">
                            <text>更多</text>
                        </view>-->
